@model IEnumerable<RMAModelVM>
@{
    Layout = null;
    var RMAReasonList = EnumData.RMAReasonList();
    var Warehouselist = EnumData.WarehouseDDL();
}
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                SKU
            </th>
            <th>
                Product Name
            </th>
            <th>
                QTY
            </th>
            <th>Return Warehouse</th>
            <th>Reason</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item.SKU
            </td>
            <td>
                @item.ProductName
            </td>
            <td>
                @item.QTY
            </td>
            <td>
                @Warehouselist[item.Warehouse.ToString()]
            </td>
            <td>
                @{ 
                    var Reason = 0;
                    int.TryParse(item.Reason, out Reason);
                }
                @RMAReasonList[Reason]
            </td>
        </tr>
        }
    </tbody>
</table>
