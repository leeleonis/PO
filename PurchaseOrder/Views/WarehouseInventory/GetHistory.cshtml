@model IEnumerable<SerialsLlist>
@{
    Layout = null;
}
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Date</th>
            <th>IS Type</th>
            <th>ID</th>
            <th>Warehouse</th>
            <th>QTY</th>
            <th>Updated By</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CreateAt</td>
                @if (item.PurchaseSKU.PurchaseOrder != null)
                {
                    <td>@item.PurchaseSKU.PurchaseOrder.POType</td>
                    <td>@item.ID</td>
                    <td>@item.PurchaseSKU.PurchaseOrder.Warehouse1.Name</td>
                }
                else if (item.PurchaseSKU.CreditMemo != null)
                {
                    <td>@item.PurchaseSKU.CreditMemo.CMType</td>
                    <td>@item.ID</td>
                    <td>@item.PurchaseSKU.CreditMemo.PurchaseOrder.Warehouse1.Name</td>
                }
                <td>@item.SerialsQTY</td>
                <td>@item.CreateBy</td>
            </tr>
        }
    </tbody>

</table>
