@{
    Layout = null;
}
@using (Ajax.BeginForm("UpSerialsExcel", "Ajax", new AjaxOptions { HttpMethod = "Post", InsertionMode = InsertionMode.Replace, UpdateTargetId = "dialog", OnSuccess = "Success" }, new { enctype = "multipart/form-data" }))
{
    @Html.Hidden("id")
    @Html.Hidden("key")
    <div class="col-sm-9 col-lg-2">
        <input type='file' class="ImgFile" name="ExcelFile" id="ExcelFile" accept="text/csv,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet">
        <br />
       <button type="submit" class="btn btn-primary" id="save">儲存</button>
        <label id="errmsg"></label>
    </div>
}

